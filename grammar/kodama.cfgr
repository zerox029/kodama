module kodama;

import dataType;

assignment : 'let' identifier ':' integer_data_type '=' add_expression;

add_expression : mul_expression '+' add_expression
               | mul_expression '-' add_expression
               | mul_expression;

mul_expression : number '*' mul_expression
               | number '/' mul_expression
               | number '%' mul_expression
               | number;

identifier: {'a'-'z' | 'A'-'Z'};
number: {'0'-'9'};